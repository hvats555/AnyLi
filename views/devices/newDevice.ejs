<%- include('../partials/header') %>
<div class="row">
    <h1 style="text-align: center">Add new Device</h1>
    <div style="width: 30%; margin: 25px auto;">
        <form action="/device" method="post">
            <div class="form-group">
                <input class="form-control" type="text" name="deviceName" placeholder="Enter device name...">
            </div>
            <div class="form-group">
                <input class="btn btn-lg btn-primary btn-block" type="submit" value="Add Device">
            </div>
        </form>
        <a href="/devices">See all devices</a>
    </div>
</div>
<%- include('../partials/footer') %>


